    "ocaml": ">=4.6.0",
    "esy-tree-sitter": "onivim/esy-tree-sitter#220f95bd",
    "reperf": "^1.3.0",
    "editor-core-types": "onivim/editor-core-types#5e5ffd9",
    "reason-textmate": "^3.0.0"
  },
  "devDependencies": {
    "ocaml": "~4.8.0"
  }
}
