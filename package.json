{
  "name": "reason-tree-sitter",
  "version": "1.0.1000",
  "description": "Reason bindings for tree-sitter",
  "license": "MIT",
  "scripts": {
    "format": "esy dune build @fmt --auto-promote"
  },
  "esy": {
    "build": ["refmterr dune build -p treesitter"],
    "buildsInSource": "_build",
    "install": [
        "esy-installer tree-sitter.install"
    ]
  },
  "resolutions": {
    "@esy-ocaml/esy-installer": "0.0.1",
    "@opam/dune": "1.7.0",
    "@opam/merlin-extend": "0.3",
    "@opam/ocaml-migrate-parsetree": "1.3.1",
    "@opam/biniou": "1.2.0",
    "@opam/easy-format": "1.3.1"
  },
  "dependencies": {
    "@opam/dune": "*",
    "@esy-ocaml/reason": "3.4.0",
    "@reason-native/rely": "2.2.0",
    "refmterr": "*",
    "ocaml": "4.7.1004",
    "esy-tree-sitter": "1.3.0",
    "reperf": "^1.3.0"
  },
  "devDependencies": {
    "ocaml": "4.7.1004"
  }
}
